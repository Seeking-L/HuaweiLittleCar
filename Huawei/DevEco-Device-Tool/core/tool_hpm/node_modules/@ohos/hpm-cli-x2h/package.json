{
  "name": "@ohos/hpm-cli-x2h",
  "version": "1.0.4",
  "description": "generate bundle.json by pom.xml|package.json, then publish to hpm ",
  "main": "./src/index.js",
  "private":false,
  "scripts": {
    "build": "node build.js",
    "test:npm": "node ./bin/x2h npm -p",
    "pack": "npm run build && npm pack",
    "test:maven": "node ./bin/x2h maven -p",
    "test:gradle": "node ./bin/x2h gradle -p"
  },
  "bin": {
    "x2h": "./bin/x2h"
  },
  "keywords": [
    "maven",
    "gradle",
    "npm",
    "hpm",
    "json",
    "xml",
    "bundle"
  ],
  "tags": [
    "js"
  ],
  "author": "raozhangwen <raoswing@qq.com>",
  "license": "Apache License 2.0",
  "dependencies": {
    "lowdb": "^2.1.0",
    "pom-parser": "^1.2.0",
    "gradle-to-js": "^2.0.0",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.9"
  }
}
