{
  "name": "@ohos/hpm-cli-ui",
  "version": "1.0.2",
  "description": "This is a gui plugin for hpm-cli",
  "private": false,
  "license": "Apache License 2.0",
  "scripts": {
    "dev:ui": "vue-cli-service serve",
    "build:ui": "vue-cli-service build",
    "dev:server": "cross-env NODE_ENV=development_api node server.build.js && hpm ui",
    "build:server": "node server.build.js",
    "build": "npm run build:ui && npm run build:server",
    "test:ui:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint src server/src --no-fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "axios": "^0.23.0",
    "bowser": "^2.10.0",
    "cookie-parser": "~1.4.5",
    "core-js": "^3.6.4",
    "element-ui": "2.13.0",
    "express": "~4.17.1",
    "https-proxy-agent": "^2.2.4",
    "lodash": "^4.17.21",
    "markdown-it": "^12.3.0",
    "morgan": "~1.9.1",
    "svgxuse": "^1.2.6",
    "vue": "^2.6.14",
    "vue-i18n": "^8.25.0",
    "vue-json-pretty": "1.8.1",
    "vue-router": "^3.5.2",
    "vuex": "^3.6.2",
    "ws": "^7.4.4",
    "xterm": "4.12.0",
    "xterm-addon-fit": "^0.5.0",
    "xterm-addon-web-links": "^0.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/preset-env": "^7.12.17",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.3.8",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "shelljs": "^0.8.4",
    "style-resources-loader": "^1.4.1",
    "svg-sprite-loader": "^5.2.1",
    "vue-template-compiler": "^2.6.14",
    "webpack-bundle-analyzer": "^4.4.0"
  },
  "files": [
    "dist/**/*"
  ]
}
